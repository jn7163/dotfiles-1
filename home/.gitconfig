[core]
	legacyheaders = false
	quotepath = false
	pager = less -+$LESS -FRX
	autocrlf = input
	safecrlf = true
	excludesfile = /Users/Havee/.gitignore_global
	editor = vim
	#pager = less -r
[user]
	name = Register
	email = registerdedicated@gmail.com
[diff]
	tool = diff
[alias]
	ll = log --graph --pretty=format:'%C(yellow)%h%Creset -%C(cyan)%d%Creset %s %Cgreen(%an, %cr)' --abbrev-commit
	l = log --stat --decorate --abbrev-commit --max-count=3
	sbi = submodule foreach --recursive git submodule init
	sbu = submodule foreach --recursive git submodule update
	sbp = submodule foreach --recursive git pull origin master
	sbc = submodule foreach --recursive git checkout master
[color]
	branch = auto
	diff = auto
	status = auto
	grep = auto
	ui = auto
	interactive = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow
	frag = magenta
	old = red
	new = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[gist]
	private = no
	browse = no
[github]
	user = Havanna
[push]
	default = matching
[web]
	browser = firefox
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /opt/homebrew-cask/Caskroom/sourcetree/2.0.5.2/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
