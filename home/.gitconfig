[core]
	legacyheaders = false
	quotepath = false
	pager = less -+$LESS -FRX
	autocrlf = input
	safecrlf = true
	excludesfile = /Users/cnhavee/.gitignore_global
	editor = vim
	#pager = less -r
[user]
	name = Register
	email = registerdedicated+github@gmail.com
[diff]
	tool = diff
[alias]
	ll= log --graph --pretty=format:'%C(yellow)%h%Creset -%C(cyan)%d%Creset %s %Cgreen(%an, %cr)' --abbrev-commit
	l = log --stat --decorate --abbrev-commit --max-count=3
	sp = submodule update --recursive --init
	sf = submodule foreach git submodule update --recursive --init
[color]
	ui = true
[gist]
	private = no
	browse = no
[github]
	user = registe
[push]
	default = simple
[pull]
	default = simple
[web]
	browser = firefox
