alias ls='ls --color=auto'
alias ll='ls -lh'
alias grep='grep --color=auro'
alias netstat='netstat -antp'
alias less='less -R'
alias route='route -n'
alias ssh='ssh -C'
alias df='df -Th'
alias du='du -sh'
alias free='free -m'
alias ping='ping -c 4'
