Xft.dpi:                96
Xft.lcdfilter:          lcddefault
Xft.antialias:          true
Xft.rgba:               rgba
Xft.hinting:            true
Xft.autohint:           true
Xft.hintstyle:          hintsfull
!! RXVT-unicode setting
URxvt.geometry:			100x24+80+80
!URxvt.background:		black
URxvt.foreground:		gray
!设置背景色
URxvt.background:		rgba:0000/0000/0000/cccc
!设置颜色深度
URxvt.depth:			32
URxvt.scrollBar:		False
URxvt.scrollstyle:		next
! Use shift+pageup/down to scroll in screen
!!URxvt.secondaryScroll:	True
!set the artificial transparency of the v.t.
!!URxvt.inheritPixmap:	True
!!URxvt.tintColor:		grey
!!URxvt.shading:		500
URxvt.saveLines:		2000
!!URxvt.borderLess:		False
!!URxvt.title:			Havee Rxvt-Unicode

!!URxvt.font:xft:DejaVu Sans Mono:pixelsize=10:antialias=False,\xft:WenQuanYi Micro Hei:pixelsize=10:antialias=False
URxvt.font:xft:DejaVu Sans Mono-10,xft:Source Han Sans CN Light-10
URxvt.boldFont:xft:DejaVu Sans Mono-10:Bold,xft:Source Han Sans CN Light-10:Bold

!!Input Method set
URxvt.inputMethod:		ibus
URxvt.preeditType:		Root
!URxvt.preeditType:		OffTheSpot
!URxvt.preeditType:		OverTheSpot

